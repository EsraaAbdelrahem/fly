ate>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\nimport { mapActions } from \"vuex\";\n\nVue.use(axios);\n\nexport default {\n  data() {\n    return {\n      listings: []\n    };\n  },\n  methods: {\n    ...mapActions({\n      getCardDetails: \"Hotels/getCardDetails\"\n    }),\n\n    getHotelListing() {\n      return axios\n        .get(\n          \"http://my-json-server.typicode.com/fly365com/code-challenge/hotels\"\n        )\n        .then((result) => {\n          this.listings = result.data;\n        });\n    }\n  },\n  created() {\n    this.getHotelListing();\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n/* I didn't do any desgin I'm just using \na very basic and generic less styling for this component \n*/ \ndiv {\n  display: flex;\n  justify-content: space-between;\n  margin: 10px;\n  .card {\n   width: 50%;\n   flex-wrap: wrap;\n   .card-content {\n     img {\n       width: 50%;\n     }\n   } \n  }\n}\n</style>\n"]}]}